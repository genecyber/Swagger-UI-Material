{{#ifCond sampleJSON "||" sampleXML}}
  <div class="signature-panel">
    <ul class="tabs">
      <li class="tab col s3"><a class="description-link active" href="#signature_{{increment}}_description">Model</a></li>
      <li class="tab col s3"><a class="snippet-link" href="#signature_{{increment}}_snippet">Example</a></li>
    </ul>
    
    <div id="signature_{{increment}}_description" class="description">
      {{{signature}}}
    </div>
    <div id="signature_{{increment}}_snippet" class="snippet">
      {{#if sampleJSON}}
          <div class="snippet_json">
            <pre><code>{{sampleJSON}}</code></pre>
            {{#if isParam}}<small class="notice" data-sw-translate></small>{{/if}}
          </div>
      {{/if}}
      {{#if sampleXML}}
        <div class="snippet_xml">
          <pre><code>{{sampleXML}}</code></pre>
          {{#if isParam}}<small class="notice" data-sw-translate></small>{{/if}}
        </div>
      {{/if}}
    </div>
  </div>
{{else}}
  <span class="propType">{{signature}}</span>
{{/ifCond}}
